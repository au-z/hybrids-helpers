{
  "name": "@auzmartist/hybrids-helpers",
  "version": "0.3.5",
  "type": "module",
  "main": "./dist/hybrids-helpers.umd.cjs",
  "module": "./dist/hybrids-helpers.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/hybrids-helpers.js",
      "require": "./dist/hybrids-helpers.umd.cjs"
    }
  },
  "files": [
    "README.md",
    "package.json",
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "dev": "vite",
    "docgen": "typedoc --out docs src/index.ts",
    "docs:preview": "pnpm docgen && pnpm dlx http-server --open -p 10000 ./docs",
    "test": "vitest test",
    "test:once": "vitest run test",
    "prepublishOnly": "pnpm build && pnpm test:once && pnpm docgen"
  },
  "dependencies": {
    "hybrids": "^8.0.8",
    "redux": "^4.2.0"
  },
  "peerDependencies": {
    "hybrids": "^8.0.0",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "happy-dom": "^7.4.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "typedoc": "^0.23.18",
    "wireit": "^0.9.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  }
}