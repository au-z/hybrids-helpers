{
  "name": "@auzmartist/hybrids-helpers",
  "version": "0.3.2",
  "type": "module",
  "main": "./dist/hybrids-helpers.umd.js",
  "module": "./dist/hybrids-helpers.es.js",
  "exports": {
    ".": {
      "import": "./dist/hybrids-helpers.es.js",
      "require": "./dist/hybrids-helpers.umd.js"
    }
  },
  "files": [
    "README.md",
    "package.json",
    "dist/**/*"
  ],
  "types": "./dist/types",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "prepublishOnly": "pnpm build && pnpm docgen",
    "test": "vitest test",
    "docgen": "typedoc --out docs src/index.ts",
    "docs:preview": "pnpm docgen && pnpm dlx http-server --open -p 10000 ./docs"
  },
  "dependencies": {
    "hybrids": "^8.0.8",
    "redux": "^4.2.0"
  },
  "peerDependencies": {
    "hybrids": "^8.0.0",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "happy-dom": "^7.4.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.16.0",
    "typedoc": "^0.23.18"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  }
}